{
"rewrites": [
{
"source": "/(.)",
"destination": "/index.html"
}
],
"headers": [
{
"source": "/(.)\.bin",
"headers": [
{
"key": "Content-Type",
"value": "application/octet-stream"
}
]
},
{
"source": "/(.*)\.json",
"headers": [
{
"key": "Content-Type",
"value": "application/json"
}
]
}
]
}